{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@upload/upload-picture": "<rootDir>/../libs/upload-picture/src",
    "@upload/upload-picture/(.*)": "<rootDir>/../libs/upload-picture/src/$1"
  }
}